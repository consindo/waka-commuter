{
  "name": "statsnz",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:js && npm run process",
    "build:wsp": "npm run build:js && npm run process:wsp && npm run shapes:wsp",
    "build:js": "rollup -c",
    "process": "npm run process:area && npm run process:travel && npm run process:vaccine && npm run process:ethnicity && npm run process:combine && npm run process:copy",
    "process:area": "node data_sources/parsers/area-to-area-parser.js",
    "process:ethnicity": "node data_sources/parsers/ethnicity-parser.js",
    "process:vaccine": "node data_sources/parsers/vaccine-parser.js",
    "process:travel": "node data_sources/parsers/workplace-travel-split-parser.js",
    "process:combine": "node data_sources/parsers/combine-files.js",
    "process:copy": "node data_sources/parsers/split-regions.js data_sources/outputs",
    "process:wsp": "npm run process:wsp:area && npm run process:wsp:combine && npm run process:wsp:copy",
    "process:wsp:area": "node commuterview-extras/data_sources/parsers/wsp/area-to-area-parser.js",
    "process:wsp:combine": "node commuterview-extras/data_sources/parsers/wsp/combine-files.js",
    "process:wsp:copy": "node data_sources/parsers/split-regions.js commuterview-extras/data_sources/outputs/wsp",
    "shapes:wsp": "ncp commuterview-extras/shapes dist/shapes/extras",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "rollup -c -w",
    "start": "sirv dist",
    "prettier": "prettier --write src/**/*.svelte src/**/*.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "lit-element": "^3.2.2",
    "mapbox-gl": "^2.12.0",
    "polylabel": "^1.1.0",
    "sirv-cli": "^2.0.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-replace": "^5.0.2",
    "csv-parser": "^3.0.0",
    "ncp": "^2.0.0",
    "prettier": "^2.8.2",
    "prettier-plugin-svelte": "^2.9.0",
    "rollup": "^3.9.1",
    "rollup-plugin-css-only": "^4.3.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-svelte": "^7.1.0",
    "@rollup/plugin-terser": "^0.3.0",
    "strip-bom-stream": "^4.0.0",
    "svelte": "^3.55.0"
  }
}
